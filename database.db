-- Новый пример таблицы
CREATE TABLE IF NOT EXISTS products (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    rostov_stock INTEGER NOT NULL,
    moscow_stock INTEGER NOT NULL
);

